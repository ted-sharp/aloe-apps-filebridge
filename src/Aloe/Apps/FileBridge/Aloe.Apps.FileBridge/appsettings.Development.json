{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "FileBridge": {
    "WatchDirectory": "C:\\Watch",
    "PollingIntervalSeconds": 30,
    "ExecutablePath": "",
    "Arguments": "{FilePath}",
    "LogDirectory": "logs",
    "LogRetentionDays": 30,
    "MaxLogsPerFile": 10000
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    }
  }
}
